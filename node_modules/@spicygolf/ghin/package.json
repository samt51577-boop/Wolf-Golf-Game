{
  "name": "@spicygolf/ghin",
  "description": "â›³ An unofficial api wrapper for the Golf Handicap and Information Network (GHIN)",
  "version": "0.8.8",
  "author": {
    "name": "Nate Clark",
    "url": "https://github.com/n8io"
  },
  "contributors": [
    {
      "name": "Brad Anderson",
      "url": "https://github.com/boorad"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boorad/ghin.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "packageManager": "bun@1.3.2",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && tsup src/index.ts --format cjs,esm --dts",
    "changeset": "npx changeset",
    "dev": "bun --bun run src/playground/ghin.ts",
    "format": "biome check --write ./src",
    "lint": "biome lint ./src && tsc",
    "release": "bun run build && npm exec changeset publish",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "dotenv": "^16.4.5",
    "husky": "^9.1.7",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "async-mutex": "^0.5.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jwt-decode": "^4.0.0",
    "neverthrow": "^8.2.0",
    "zod": "^3.22.4"
  }
}
